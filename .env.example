## Local environment variables
## Copy this file to .env and fill in your project values:
##   cp .env.example .env

# Your Vercel production URL (no trailing slash)
VERCEL_URL="https://YOUR-PROJECT.vercel.app"

# Vercel Deploy Hook URL (create in Vercel Dashboard → Settings → Git → Deploy Hooks)
VERCEL_DEPLOY_HOOK=""

# Local SSL for dev server (requires mkcert certs in ./certs). Usually false.
USE_SSL=false
